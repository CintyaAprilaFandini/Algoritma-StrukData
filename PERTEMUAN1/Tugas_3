
import java.util.Scanner;
public class Tugas_3 {
    public static void main(String[] args){
    Scanner input = new Scanner(System.in); 
    int c = 0, n; 
    int i;          
    System.out.print("Masukkan Deret N : "); 
    c = input.nextInt(); 
 	for (i = 0; i <= c * 2; i++) { 
	    n = 2 * i; 
 	    if (n % 4 == 0) { 
                System.out.print(" "); 
 	   } 
            else { 
                System.out.println("" + n); 
	   } 
        } 
    } 
 }
